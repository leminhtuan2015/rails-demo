<H1></H1>

<span>
<%= link_to "Back to my home", user_path%>
</span>

<div class="detailBox">
<table class="table">
      <caption>Users</caption>
      <thead>
        <tr>
          <th>#</th>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
      <% @users.each do |user| %>
        <tr>
          <th scope="row"><%=user.id%></th>
          <td><%= link_to user.name, user_path(user)%></td>
          <td><%= user.email %></td>
        </tr>
       <% end %> 
      </tbody>
    </table>
</div>